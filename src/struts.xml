<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="control" extends="struts-default">
	
		<action name="pessoa.consultar" class="control.PessoaAction" method="consultar">
			<result name="success">/pessoa.jsp</result>
		</action>
		
		<action name="pessoa.cadastrar" class="control.PessoaAction" method="cadastrar">
			<result name="success">/pessoa.jsp</result>
			<result name="error">/pessoa.jsp</result>
		</action>
	
	</package>
	

</struts>
